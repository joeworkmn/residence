<tr id='<%= dom_id(user) %>'>
   <td> </td>
   <td> <%= user.name_reversed %> </td>
   <td> <%= user.email %> </td>
   <td> <%= user.phone_primary %> </td>
   <td> <%= user.phone_secondary %> </td>
   <td> <%= user.roles.join(', ') %> </td>
   <td> 
      <%= link_to "edit", edit_user_path(user) %> |
      <%= link_to "delete", user, method: 'delete', remote: true, data: { confirm: "Really delete?" } %> 
   </td>
</tr>
